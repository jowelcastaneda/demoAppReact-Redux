{
  "name": "dppr-ui",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:8080",
  "scripts": {
    "start": "react-scripts start webpack-dev-server --config config/webpack.config.dev.js",
    "build": "react-scripts build rm -rf build && webpack --config config/webpack.config.build.js",
    "test": "CI=1 react-scripts test --coverage --env=jsdom",
    "test-nowatch": "react-scripts test --watchAll=false",
    "test-watch": "react-scripts test --watchAll=true",
    "test-coverage": "react-scripts test --coverage --watchAll=false",
    "test-coverage-watch": "react-scripts test --watchAll=true --coverage",
    "test-debug": "react-scripts --inspect-brk test --runInBand --no-cache",
    "lint": "eslint src --fix",
    "ci-test": "CI=true yarn test --coverage",
    "sonar": "node sonar-project.js --inspect",
    "startServer": "serve -C -d -s build"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@tensorflow-models/coco-ssd": "^2.1.0",
    "axios": "^0.21.1",
    "connected-react-router": "^6.8.0",
    "core-js": "^3.7.0",
    "face-api.js": "^0.22.2",
    "history": "^5.0.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.0",
    "redux": "^4.0.5",
    "redux-saga": "^1.1.3"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@bugsnag/js": "^6.5.2",
    "@testing-library/react": "^9.5.0",
    "@testing-library/react-hooks": "^3.4.2",
    "@testing-library/user-event": "^7.2.1",
    "babel-eslint": "^10.1.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.21.4",
    "eslint-plugin-standard": "^4.0.1",
    "node-sass": "^4.14.1",
    "redux-thunk": "^2.2.0",
    "redux-form": "^7.3.0",
    "redux-mock-store": "^1.5.4",
    "redux-promise": "^0.5.3",
    "redux-promise-middleware": "^5.0.0",
    "redux-saga-test-plan": "^4.0.1",
    "redux-devtools-extension": "^2.13.8",
    "redux-logger": "^3.0.6",
    "serve": "^11.3.2",
    "sonarqube-scanner": "^2.7.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "compression-webpack-plugin": "^3.1.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
